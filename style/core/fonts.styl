@font-face {
    font-family: 'Gotham';
    src: url('../static/fonts/gotham-bold-webfont.eot');
    src: url('../static/fonts/gotham-bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('../static/fonts/gotham-bold-webfont.woff2') format('woff2'),
         url('../static/fonts/gotham-bold-webfont.woff') format('woff'),
         url('../static/fonts/gotham-bold-webfont.ttf') format('truetype'),
         url('../static/fonts/gotham-bold-webfont.svg#gotham_boldregular') format('svg');
    font-weight: 900;
    font-style: normal;
}

